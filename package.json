{
  "name": "boilerplate-vue2",
  "version": "0.0.0",
  "private": true,
  "description": "A boilerplate for vue-cli and vue2.",
  "scripts": {
    "build": "vue-cli-service build --mode production --report",
    "build:staging": "vue-cli-service build --mode staging --report",
    "check:deps": "ncu",
    "check:types": "vue-tsc",
    "commit": "cz",
    "dev": "vue-cli-service serve --mode development",
    "lint": "pnpm run lint:eslint && pnpm run lint:markdownlint && pnpm run lint:stylelint",
    "lint:eslint": "vue-cli-service lint . --fix --ext=.js,.jsx,.ts,.tsx,.vue --ignore-path=.gitignore",
    "lint:markdownlint": "markdownlint . --fix --ignore-path=.gitignore",
    "lint:stylelint": "stylelint \"./**/*.{css,less,scss,vue}\" --fix --allow-empty-input --ignore-path=.gitignore",
    "prepare": "is-ci || husky install",
    "release": "release-it"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@ant-design/colors": "~6.0.0",
    "@iconify/vue2": "~1.0.0",
    "@modyqyw/utils": "~2.0.1",
    "@sum.cumo/vue-browserupdate": "~3.0.1",
    "@vue/composition-api": "~1.4.0",
    "@vueuse/core": "~6.9.1",
    "@vueuse/integrations": "~6.9.1",
    "axios": "~0.24.0",
    "axios-cache-adapter": "~2.7.3",
    "axios-logger": "~2.6.0",
    "axios-mock-adapter": "~1.20.0",
    "axios-retry": "~3.2.4",
    "core-js": "~3.19.1",
    "dayjs": "~1.10.7",
    "element-ui": "~2.15.6",
    "faker": "~5.5.3",
    "mitt": "~3.0.0",
    "mockjs": "~1.1.0",
    "modern-normalize": "~1.1.0",
    "nprogress": "~0.2.0",
    "portal-vue": "~2.1.7",
    "statuses": "~2.0.1",
    "tailwindcss": "~1.9.6",
    "vue": "~2.6.14",
    "vue-auto-routing": "~1.0.1",
    "vue-query": "~1.14.0",
    "vue-router": "~3.5.3",
    "vue-router-layout": "~0.1.5",
    "vue2-helpers": "~1.1.6",
    "vuex": "~3.6.2"
  },
  "devDependencies": {
    "@babel/core": "~7.16.0",
    "@babel/eslint-parser": "~7.16.3",
    "@commitlint/cli": "~15.0.0",
    "@iconify/json": "~1.1.429",
    "@modyqyw/fabric": "~4.0.0-17",
    "@types/faker": "~5.5.9",
    "@types/mockjs": "~1.0.4",
    "@types/node": "~16.11.7",
    "@types/nprogress": "~0.2.0",
    "@types/statuses": "~2.0.0",
    "@typescript-eslint/eslint-plugin": "~5.4.0",
    "@typescript-eslint/parser": "~5.4.0",
    "@vue/cli-plugin-babel": "~5.0.0-rc.1",
    "@vue/cli-plugin-eslint": "~5.0.0-rc.1",
    "@vue/cli-plugin-router": "~5.0.0-rc.1",
    "@vue/cli-plugin-typescript": "~5.0.0-rc.1",
    "@vue/cli-plugin-vuex": "~5.0.0-rc.1",
    "@vue/cli-service": "~5.0.0-rc.1",
    "@vue/runtime-dom": "~3.2.22",
    "@vue/test-utils": "~1.3.0",
    "commitizen": "~4.2.4",
    "compression-webpack-plugin": "~9.0.0",
    "eslint": "~7.32.0",
    "husky": "~7.0.4",
    "is-ci": "~3.0.1",
    "lint-staged": "~12.0.2",
    "markdownlint-cli": "~0.29.0",
    "npm-check-updates": "~12.0.2",
    "postcss-preset-env": "~7.0.0",
    "prettier": "~2.2.1",
    "release-it": "~14.11.7",
    "rimraf": "~3.0.2",
    "sass": "~1.32.13",
    "sass-loader": "~12.3.0",
    "stylelint": "~14.1.0",
    "stylelint-webpack-plugin": "~3.1.0",
    "typescript": "~4.4.4",
    "unplugin-icons": "~0.12.18",
    "unplugin-vue-components": "~0.17.2",
    "unplugin-vue2-script-setup": "~0.6.16",
    "vue-cli-plugin-auto-routing": "~1.3.0",
    "vue-template-compiler": "~2.6.14",
    "vue-tsc": "~0.29.5"
  }
}
