{
  "name": "boilerplate-vue2",
  "version": "0.0.0",
  "private": true,
  "description": "A boilerplate for vue-cli and vue2.",
  "scripts": {
    "build": "vue-cli-service build --mode production --report",
    "check": "ncu",
    "commit": "cz",
    "dev": "vue-cli-service serve --mode development",
    "i18n:report": "vue-cli-service i18n:report --src './src/**/*.?(js|vue)' --locales './src/i18n/locales/*.json'",
    "lint": "yarn run lint:eslint && yarn run lint:markdownlint && yarn run lint:stylelint",
    "lint:eslint": "vue-cli-service lint --fix",
    "lint:markdownlint": "markdownlint . --fix --ignore-path=.gitignore",
    "lint:stylelint": "stylelint ./**/*.{css,less,scss,vue} --fix --allow-empty-input --ignore-path=.gitignore",
    "prepare": "is-ci || husky install",
    "release": "release-it",
    "staging-build": "vue-cli-service build --mode staging --report"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@ant-design/colors": "~6.0.0",
    "@iconify/vue2": "~1.0.0",
    "@modyqyw/utils": "~1.0.0",
    "@sum.cumo/vue-browserupdate": "~3.0.1",
    "@tailwindcss/typography": "~0.2.0",
    "@vue/composition-api": "~1.1.5",
    "@vueuse/core": "~6.3.3",
    "@vueuse/gesture": "~1.0.0",
    "@vueuse/integrations": "~6.3.3",
    "@vueuse/motion": "~1.6.0",
    "axios": "~0.21.4",
    "axios-cache-adapter": "~2.7.3",
    "axios-logger": "~2.5.0",
    "axios-mock-adapter": "~1.20.0",
    "axios-retry": "~3.1.9",
    "core-js": "~3.17.3",
    "dayjs": "~1.10.7",
    "element-ui": "~2.15.6",
    "faker": "~5.5.3",
    "mitt": "~3.0.0",
    "mockjs": "~1.1.0",
    "nprogress": "~0.2.0",
    "portal-vue": "~2.1.7",
    "ress": "~4.0.0",
    "statuses": "~2.0.1",
    "tailwindcss": "~1.9.6",
    "vue": "~2.6.14",
    "vue-auto-routing": "~1.0.1",
    "vue-i18n": "~8.25.0",
    "vue-router": "~3.5.2",
    "vue-router-layout": "~0.1.5",
    "vuex": "~3.6.2"
  },
  "devDependencies": {
    "@babel/core": "~7.15.5",
    "@babel/eslint-parser": "~7.15.4",
    "@commitlint/cli": "~13.1.0",
    "@fullhuman/postcss-purgecss": "~4.0.3",
    "@fullhuman/vue-cli-plugin-purgecss": "~4.0.3",
    "@iconify/json": "~1.1.400",
    "@intlify/vue-i18n-loader": "~1.1.0",
    "@modyqyw/fabric": "~3.4.3",
    "@types/faker": "~5.5.8",
    "@types/mockjs": "~1.0.4",
    "@types/node": "~16.9.1",
    "@types/nprogress": "~0.2.0",
    "@types/statuses": "~2.0.0",
    "@typescript-eslint/eslint-plugin": "~4.31.0",
    "@typescript-eslint/parser": "~4.31.0",
    "@vue/cli-plugin-babel": "~5.0.0-beta.3",
    "@vue/cli-plugin-eslint": "~5.0.0-beta.3",
    "@vue/cli-plugin-router": "~5.0.0-beta.3",
    "@vue/cli-plugin-typescript": "~5.0.0-beta.3",
    "@vue/cli-plugin-vuex": "~5.0.0-beta.3",
    "@vue/cli-service": "~5.0.0-beta.3",
    "@vue/runtime-dom": "~3.2.11",
    "@vue/test-utils": "~1.2.2",
    "commitizen": "~4.2.4",
    "compression-webpack-plugin": "~9.0.0",
    "eslint": "~7.32.0",
    "husky": "~7.0.2",
    "is-ci": "~3.0.0",
    "less": "~4.1.1",
    "less-loader": "~10.0.1",
    "lint-staged": "~11.1.2",
    "markdownlint-cli": "~0.28.1",
    "npm-check-updates": "~11.8.5",
    "postcss-preset-env": "~6.7.0",
    "prettier": "~2.2.1",
    "release-it": "~14.11.5",
    "sass": "~1.32.13",
    "sass-loader": "~12.1.0",
    "stylelint": "~13.13.1",
    "stylelint-webpack-plugin": "~3.0.1",
    "typescript": "~4.4.3",
    "unplugin-icons": "~0.8.1",
    "unplugin-vue-components": "~0.15.0",
    "unplugin-vue2-script-setup": "~0.6.1",
    "vue-cli-plugin-auto-routing": "~1.3.0",
    "vue-template-compiler": "~2.6.14"
  },
  "engines": {
    "node": "^12.22.6 || ^14.17.6 || ^16.6.2",
    "npm": "^6.14.15 || ^7.20.3"
  }
}
